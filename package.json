{
  "name": "genme",
  "description": "Generate to me the boring code",
  "version": "0.0.0",
  "author": "KLC215 @KLC215",
  "bin": {
    "genme": "./bin/run"
  },
  "bugs": "https://github.com/KLC215/genme/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "bluebird": "^3.5.2",
    "cli-ux": "^4.8.2",
    "edge.js": "^1.1.4",
    "mysql": "mysqljs/mysql",
    "pluralize": "^7.0.0",
    "tslib": "^1",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10",
    "@types/pluralize": "^0.0.29",
    "@types/voca": "^1.4.0",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/KLC215/genme",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "genme",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "KLC215/genme",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
